#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dhlee/Documents/react-study/our-film/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/dhlee/Documents/react-study/our-film/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tsconfck@2.1.1_typescript@5.1.3/node_modules/tsconfck/bin/tsconfck.js" "$@"
else
  exec node  "$basedir/../../../../../tsconfck@2.1.1_typescript@5.1.3/node_modules/tsconfck/bin/tsconfck.js" "$@"
fi
